#!/usr/bin/env groovy
properties([
    [$class: 'GithubProjectProperty',
    displayName: '',
    projectUrlStr: 'https://github.com/saipradeep41/Upstream_Downstream_Multibranch_Pipeline_Task7.git'],
    pipelineTriggers([githubPush()])])
    pipeline {
    agent any 

    stages {
        stage('Build') { 
            steps { 
                echo 'pwd' 
            }
        }
        stage('Test'){
            steps {
                echo 'saipradeep'
                
            }
        }
        stage('Deploy') {
            steps {
                echo 'saipradeep'
                echo 'pwd'
            }
        }
    }
}
