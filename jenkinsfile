#!/usr/bin/env groovy
properties([
    [$class: 'GithubProjectProperty',
    displayName: '',
    projectUrlStr: 'https://github.com/VeridicSolutions99/Veridic_Atlanta.git/'],
    pipelineTriggers([githubPush()])])
    pipeline {
    agent any 

    stages {
        stage('Build') { 
            steps { 
                echo 'pwd' 
            }
        }
        stage('Test'){
            steps {
                echo 'java -version'
                
            }
        }
        stage('Deploy') {
            steps {
                echo 'ls'
                echo 'pwd'
            }
        }
    }
}
